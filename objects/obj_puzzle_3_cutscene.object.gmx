            <string>var caveman = inst_E34B297A;

/////////////////////
// Shell Meets Man //
/////////////////////
if flag[0] = 0{
    with obj_shell{
        if place_meeting(x,y,caveman){
            obj_puzzle_3_cutscene.flag[0] = -1;
            obj_puzzle_3_cutscene.flag[1] = 1;
            inCutscene = 1;
            caveman.state = "shell";
            camera_target = caveman;
            caveman.emotion = -1;
            instance_destroy();}}} 
            
///////////
// Delay //
///////////
if flag[1] &gt; 0{
    flag[1] ++;
    if flag[1] = 90{
        caveman.emotion = spr_lightbulb;}
    if flag[1] = 180{
        caveman.emotion = -1;
        caveman.state = ""}
    if flag[1] = 255{
        flag[2] = 1;
        flag[1] = -1;
        flag[3] = 1}}
        
//////////////
// Spinning //
//////////////
if flag[2] &gt; 0{
    if flag[2] &lt; 4 then flag[2] += 1/10;
    else flag[2] += 0.334;
    if floor(flag[2]) = 13{
        caveman.rotation = 90;
        caveman.hspeed = -5;}
    var xs = floor(flag[2]) mod 2;
    if xs = 0 then xs = -1;
    caveman.image_xscale = xs;}
    
/////////////////
// Hit la wall //
/////////////////
with caveman{
    var deactivateList;
    deactivateList[0] = 0;
    var i = -1;
    while place_meeting(x,y, obj_breakable_wall){
        var breakable = instance_place(x,y,obj_breakable_wall);
        i++;
        deactivateList[i] = breakable;
        if breakable.image_speed = 0{
            breakable.image_speed = 1/6;
            part_particles_create(global.P_System, breakable.x, breakable.y, global.p1, 50);}
        instance_deactivate_object(breakable);}
        while i &gt; -1{
            instance_activate_object(deactivateList[i]);
            i--;}}
    
///////////////
// Meet girl //
///////////////
if flag[3] = 1{
    with caveman{
        if place_meeting(x,y,obj_cavewoman){
            hspeed = 0;
            obj_cavewoman.state = "standing";
            with obj_puzzle_3_cutscene{
                flag[2] = -1;
                flag[3] = -1
                flag[4] = 1;}}}}
                
///////////
// Delay //
///////////
if flag[4] &gt; 0{
    flag[4]++;
    if flag[4] = 60{
        caveman.rotation = 0;
        caveman.image_xscale = -1;}
    if flag[4] = 120{
        obj_fade.fadeState = "out";
        flag[4] = -1;
        flag[5] = 1;}}
        
//////////
// Fade //
//////////
if flag[5] &gt; 0 and obj_fade.fadeState != "out"{
    flag[5] ++;
    if flag[5] = 2{
        camera_target = obj_player_parent;
        with caveman{
            instance_destroy();}
        with obj_cavewoman{
            instance_destroy();}
        with obj_fish{
            instance_destroy();}}
    if flag[5] = 90{
        inCutscene = 0;
        obj_player_parent.upgradeState = "drill"; 
        obj_player_parent.whiteFlash = 1;
        flag[5] = -1;
        instance_destroy();}}
</string>
