//Panic
if state = "panic"{
    image_speed = 1/30;
    sprite_index = spr_caveman_panic;}
    
//Pushing
if state = "pushing"{
    image_speed = 1/9;
    sprite_index = spr_caveman_push;
    emotion = spr_frustration_cloud;
    emotionLoop = 1;
    emotionSpeed = 30;}
    
//Climbing
if state = "climbing"{
    image_speed = 1/9;
    sprite_index = spr_caveman_climb;}

//Angry
if state = "angry"{
    image_speed = 1/30;
    sprite_index = spr_caveman_angry;
    emotion = spr_frustration_cloud;
    emotionLoop = 1;
    emotionSpeed = 30;}

//Standing
if state = "standing"{
    image_speed = 1/9;
    sprite_index = spr_caveman_fish;}
    
//Walking
if state = "walking"{
    image_speed = 1/6;
    sprite_index = spr_caveman_fish_walk;
    emotion = -1;}
    
//Shell
if state = "shell"{
    sprite_index = spr_caveman_shell;
    image_speed = 0;
    image_index = 0;}
    
if emotion = spr_lightbulb then emotionLoop = 0;
else emotionLoop = 1;

if emotion = -1 then emotionTimer = 0;

///////////////////    
// Emotion Timer //
///////////////////
if emotion != -1{
    var frames = 0;
    if emotionSpeed != 0 then frames = 1/emotionSpeed;
    emotionTimer += frames;
    if emotionTimer + frames &gt;= sprite_get_number(emotion){
        if emotionLoop = 1 then emotionTimer = 0;
        else{
            emotionSpeed = 0;}}
    if emotionLoop = 0{
        if emotionFade = -1 then emotionFade = 90;
        if emotionFade &gt; 0 then emotionFade --;
        else{
            emotionFade = -1;
            emotionTimer = 0;
            emotion = -1;}}}
</string>
